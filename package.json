{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:fun": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Anderson Rocha",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^3.3.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "config": "^3.3.11",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.3.2"
  }
}
